{
  "name": "ember-body-class",
  "version": "2.0.0",
  "description": "Easily add CSS classes to the body, including route names and loading/error states.",
  "keywords": [
    "ember-addon",
    "ember",
    "ember-cli",
    "CSS",
    "class",
    "body",
    "loading",
    "error"
  ],
  "homepage": "https://github.com/mansona/ember-body-class",
  "repository": {
    "type": "git",
    "url": "https://github.com/mansona/ember-body-class"
  },
  "license": "MIT",
  "contributors": [
    "Chris Manson <chris@manson.ie>",
    "Add Jam (https://addjam.com/)"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "changelog": "auto-changelog --template changelog.template --unreleased-only --prepend --load-github-issue-data --github-cache-dir .changelog",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "ember-compatibility-tests": "ember try:each"
  },
  "dependencies": {
    "ember-cli-babel": "^7.22.1",
    "ember-get-config": "^0.5.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "auto-changelog": "github:mansona/auto-changelog#epic",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.1.2",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.22.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^2.2.3",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fastboot-addon-tests": "^0.5.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.22.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.11.0",
    "eslint-plugin-ember": "^9.3.0",
    "eslint-plugin-node": "^11.1.0",
    "lint-to-the-future": "^0.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.5.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
